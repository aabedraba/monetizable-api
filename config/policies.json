{
  "policies": [
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "authorization",
              "value": "Bearer $env(ZAPI_KEY)",
              "overwrite": true
            }
          ]
        }
      },
      "name": "add-zapi-key-auth",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "Auth0JwtInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false,
          "auth0Domain": "$env(AUTH0_DOMAIN)",
          "audience": "$env(AUTH0_AUDIENCE)"
        }
      },
      "name": "auth0-jwt-auth-inbound",
      "policyType": "auth0-jwt-auth-inbound"
    },
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false
        }
      },
      "name": "api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/handlers/paid-request-handler)"
      },
      "name": "paid-request",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/handlers/stripe/has-active-subscription)"
      },
      "name": "stripe-active-subscription",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "CompositeInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "policies": [
            "api-key-inbound",
            "paid-request"
          ]
        }
      },
      "name": "composite-paid-request",
      "policyType": "composite-inbound"
    },
    {
      "handler": {
        "export": "CompositeInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "policies": [
            "auth0-jwt-auth-inbound",
            "stripe-active-subscription",
            "add-zapi-key-auth"
          ]
        }
      },
      "name": "composite-add-zapi-key",
      "policyType": "composite-inbound"
    }
  ]
}